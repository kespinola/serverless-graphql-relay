{
  "name": "serverless-prism",
  "custom": {
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": [
        "Content-Type",
        "X-Amz-Date",
        "Authorization",
        "X-Api-Key"
      ]
    }
  },
  "plugins": [
    "serverless-runtime-babel",
    "serverless-cors-plugin",
    "serverless-offline"
  ]
}
